          "refId": "B"
        },
        {
          "expr": "sqlalchemy_pool_size{job=\"quality-governance\"}",
          "legendFormat": "Pool Size",
          "refId": "C"
        },
        {
          "expr": "sqlalchemy_pool_overflow{job=\"quality-governance\"}",
          "legendFormat": "Overflow",
          "refId": "D"
        }
      ]
